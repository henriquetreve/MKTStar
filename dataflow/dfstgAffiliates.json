{
	"name": "dfstgAffiliates",
	"properties": {
		"folder": {
			"name": "2 - Stage"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "tblsrcAffiliates",
						"type": "DatasetReference"
					},
					"name": "Source"
				},
				{
					"dataset": {
						"referenceName": "tblstgAffiliates",
						"type": "DatasetReference"
					},
					"name": "Stage"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "SortSource"
				},
				{
					"name": "NewSource"
				},
				{
					"name": "SortStage"
				},
				{
					"name": "NewStage"
				},
				{
					"name": "Actives"
				},
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Id as integer,",
				"          Nome as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Source",
				"source(output(",
				"          Id as integer,",
				"          BK as integer,",
				"          Nome as string,",
				"          DataInicio as date,",
				"          DataFim as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Stage",
				"Source sort(asc(Id, true)) ~> SortSource",
				"SortSource select(mapColumn(",
				"          Id_SRC = Id,",
				"          Nome_SRC = Nome",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> NewSource",
				"Actives sort(asc(BK, true)) ~> SortStage",
				"SortStage select(mapColumn(",
				"          Id_STG = Id,",
				"          BK_STG = BK,",
				"          Nome_STG = Nome,",
				"          DataInicio_STG = DataInicio,",
				"          DataFim_STG = DataFim",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> NewStage",
				"Stage filter(isNull(DataFim)) ~> Actives",
				"NewSource join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1"
			]
		}
	}
}