{
	"name": "dfExtAfilliatesTgt",
	"properties": {
		"folder": {
			"name": "1 - Extration"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dsExtSalesHotmartExit",
						"type": "DatasetReference"
					},
					"name": "Sales"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Produto as string,",
				"          Afiliado as string,",
				"          {Nome do Produtor} as string,",
				"          {Documento do Produtor} as string,",
				"          {Meio de Pagamento} as string,",
				"          {Preço do Produto} as string,",
				"          {Preço da Oferta} as string,",
				"          {Tipo de Pagamento} as string,",
				"          {Tipo pagamento oferta} as string,",
				"          Moeda as string,",
				"          {Número da Parcela} as string,",
				"          {Recorrência} as string,",
				"          {Data de Venda} as string,",
				"          {Data de Confirmação} as string,",
				"          Status as string,",
				"          DDD as string,",
				"          Cidade as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Sales",
				"Sales select(mapColumn(",
				"          Produto,",
				"          Afiliado,",
				"          {Nome do Produtor},",
				"          {Documento do Produtor},",
				"          {Meio de Pagamento},",
				"          {Preço do Produto},",
				"          {Preço da Oferta},",
				"          {Tipo de Pagamento},",
				"          {Tipo pagamento oferta},",
				"          Moeda,",
				"          {Número da Parcela},",
				"          {Recorrência},",
				"          {Data de Venda},",
				"          {Data de Confirmação},",
				"          Status,",
				"          DDD,",
				"          Cidade",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}