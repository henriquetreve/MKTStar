{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfBImktstar-dev"
		},
		"dlMktStar_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'dlMktStar'"
		},
		"srvBIMKTstar_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'srvBIMKTstar'"
		},
		"srvbimktstar2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'srvbimktstar2'"
		},
		"dlMktStar_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://samktstar.dfs.core.windows.net/"
		},
		"srvBIMKTstar_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "srvbimktstarht.database.windows.net"
		},
		"srvBIMKTstar_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbBImktstars"
		},
		"srvBIMKTstar_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsql"
		},
		"srvbimktstar2_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "srvbimktstarht.database.windows.net"
		},
		"srvbimktstar2_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbBImktstars"
		},
		"srvbimktstar2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsql"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfBImktstar-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"dlMktStar_accountKey": {
						"value": "[parameters('dlMktStar_accountKey')]"
					},
					"srvBIMKTstar_password": {
						"value": "[parameters('srvBIMKTstar_password')]"
					},
					"srvbimktstar2_password": {
						"value": "[parameters('srvbimktstar2_password')]"
					},
					"dlMktStar_properties_typeProperties_url": {
						"value": "[parameters('dlMktStar_properties_typeProperties_url')]"
					},
					"srvBIMKTstar_properties_typeProperties_server": {
						"value": "[parameters('srvBIMKTstar_properties_typeProperties_server')]"
					},
					"srvBIMKTstar_properties_typeProperties_database": {
						"value": "[parameters('srvBIMKTstar_properties_typeProperties_database')]"
					},
					"srvBIMKTstar_properties_typeProperties_userName": {
						"value": "[parameters('srvBIMKTstar_properties_typeProperties_userName')]"
					},
					"srvbimktstar2_properties_typeProperties_server": {
						"value": "[parameters('srvbimktstar2_properties_typeProperties_server')]"
					},
					"srvbimktstar2_properties_typeProperties_database": {
						"value": "[parameters('srvbimktstar2_properties_typeProperties_database')]"
					},
					"srvbimktstar2_properties_typeProperties_userName": {
						"value": "[parameters('srvbimktstar2_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}