{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfBIMKTstar"
		},
		"srvBIMKTstar_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'srvBIMKTstar'"
		},
		"srvbimktstar2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'srvbimktstar2'"
		},
		"srvBIMKTstar_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "srvbimktstarht.database.windows.net"
		},
		"srvBIMKTstar_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbBImktstars"
		},
		"srvBIMKTstar_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsql"
		},
		"srvbimktstar2_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "srvbimktstarht.database.windows.net"
		},
		"srvbimktstar2_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbBImktstars"
		},
		"srvbimktstar2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsql"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/srvBIMKTstar')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"server": "[parameters('srvBIMKTstar_properties_typeProperties_server')]",
					"database": "[parameters('srvBIMKTstar_properties_typeProperties_database')]",
					"encrypt": "mandatory",
					"trustServerCertificate": false,
					"authenticationType": "SQL",
					"userName": "[parameters('srvBIMKTstar_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('srvBIMKTstar_password')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/srvbimktstar2')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"server": "[parameters('srvbimktstar2_properties_typeProperties_server')]",
					"database": "[parameters('srvbimktstar2_properties_typeProperties_database')]",
					"encrypt": "mandatory",
					"trustServerCertificate": false,
					"authenticationType": "SQL",
					"userName": "[parameters('srvbimktstar2_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('srvbimktstar2_password')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/dstblOriginal')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "srvbimktstar2",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "id",
						"type": "int",
						"precision": 10
					},
					{
						"name": "name",
						"type": "varchar"
					},
					{
						"name": "description",
						"type": "varchar"
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "origin"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/srvbimktstar2')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/dstblBackup')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "srvbimktstar2",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "id",
						"type": "int",
						"precision": 10
					},
					{
						"name": "name",
						"type": "varchar"
					},
					{
						"name": "description",
						"type": "varchar"
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "backup"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/srvbimktstar2')]"
			]
		}
	]
}